---
layout: layouts/base.njk
titleimage: /img/logo/cinnamonlogo.png
backgroundimage: /img/background/cinnamonbackground2.jpg
navbarcolor: rgb(240, 240, 240)
navlogo: emojicats
---
<main>





<div class="container">
<img class="cinnatitle" style="margin-top:20px;" src="/img/logo/cinnamoncomictitle.png"/>
</div>
<img class="comicpage" src="/img/{{ title }}/{{ chapter }}-{{ pagenumber }}.jpg"/>
<div class="container">
{% include "components/cinnamonnav.njk" %}
<div class="mangapage" style="grid-template-columns: 1fr 5fr;" >
<div class="mangahalftone"></div>
<div class="mangacontent mangapaper">
<h1>{{ title }} {{chapter}}.{{ pagenumber }}</h1>
<p class="datedisplay"> <small>{{ date }}</small> </p>
{{ content | safe }}
</div>
</div>
<div id="disqus_thread"></div>

<script>
var disqus_config = function () {
this.page.url = {{ page.url | url }};  // Replace PAGE_URL with your page's canonical URL variable
this.page.identifier = {{page.title}}-{{page.chaper}}-{{page.pagenumber}}; // Replace PAGE_IDENTIFIER with your page's unique identifier variable
};

(function() { // DON'T EDIT BELOW THIS LINE
var d = document, s = d.createElement('script');
s.src = 'https://blackbit-1.disqus.com/embed.js';
s.setAttribute('data-timestamp', +new Date());
(d.head || d.body).appendChild(s);
})();
</script>
<noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
                            
</div>
</main>
